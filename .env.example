# Worpx Protocol - Environment Variables
# Copy this file to .env and fill in your values

# API Key (required)
WORPX_API_KEY=your_api_key_here

# Wallet Configuration (optional, for self-custody mode)
WALLET_PRIVATE_KEY=0x_your_private_key_here

# Server Configuration
PORT=5000
NODE_ENV=development

# Database (PostgreSQL)
DATABASE_URL=postgresql://user:password@localhost:5432/worpx

# Session Secret
SESSION_SECRET=your_session_secret_here

# Chain RPC URLs (optional overrides)
BASE_RPC_URL=https://mainnet.base.org
ETHEREUM_RPC_URL=https://eth.llamarpc.com
POLYGON_RPC_URL=https://polygon-rpc.com
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Agent Integration
AGENT_HEARTBEAT_INTERVAL=30000
AGENT_SESSION_TTL=3600000
MAX_CONCURRENT_CHANNELS=50
